<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="5.0" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="400.0" VBox.vgrow="ALWAYS">
         <children>
            <AnchorPane fx:id="vuPane" prefWidth="50.0" />
            <VBox fx:id="bleedTopPane" alignment="BOTTOM_CENTER" prefWidth="20.0" styleClass="vuMeter-background" stylesheets="@../../css/style.css">
               <children>
                  <Pane fx:id="bleedPane" prefHeight="50.0" style="-fx-background-color: #992200;" />
               </children>
            </VBox>
         </children>
      </HBox>
      <ComboBox fx:id="combo" prefWidth="100.0" />
   </children>
</VBox>
