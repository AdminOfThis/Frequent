<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.BleedViewController">
   <center>
      <HBox fx:id="content" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <HBox onMouseClicked="#click" prefWidth="50.0" style="-fx-border-color: red;" HBox.hgrow="SOMETIMES" />
            <HBox onMouseClicked="#click" prefWidth="50.0" style="-fx-border-color: red;" HBox.hgrow="SOMETIMES" />
            <HBox onMouseClicked="#click" prefWidth="50.0" style="-fx-border-color: red;" HBox.hgrow="SOMETIMES" />
            <HBox onMouseClicked="#click" prefWidth="50.0" style="-fx-border-color: red;" HBox.hgrow="SOMETIMES" />
            <HBox onMouseClicked="#click" prefWidth="50.0" style="-fx-border-color: red;" HBox.hgrow="SOMETIMES" />
         </children>
      </HBox>
   </center>
   <top>
      <HBox fx:id="topBox" maxHeight="0.0" minHeight="0.0" prefHeight="0.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="primaryCombo" prefWidth="150.0" />
            <Button mnemonicParsing="false" onAction="#addSecondary" prefWidth="70.0" text="+" />
         </children>
      </HBox>
   </top>
   <left>
      <HBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane fx:id="primaryVuPane" minWidth="20.0" prefWidth="50.0" />
            <Separator orientation="VERTICAL" prefHeight="200.0" />
         </children>
      </HBox>
   </left>
</BorderPane>
