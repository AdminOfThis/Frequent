<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="chartRoot" minHeight="200.0" minWidth="300.0" style="-fx-border-style: none;" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.FFTController">
   <children>
      <VBox>
         <children>
            <StackPane fx:id="vuPane" prefWidth="40.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="BOTTOM_CENTER">
                     <children>
                        <Pane fx:id="vuPeakPane" prefHeight="0.0" />
                     </children>
                  </VBox>
                  <VBox alignment="BOTTOM_CENTER">
                     <children>
                        <Pane prefHeight="5.0" style="-fx-background-color: derive(-fx-accent, -50%);" />
                        <Pane fx:id="vuLastPeakPane" prefHeight="0.0" />
                     </children>
                  </VBox>
               </children>
            </StackPane>
            <AnchorPane fx:id="clippingPane" maxWidth="50.0" prefHeight="45.0" prefWidth="50.0">
               <children>
                  <Label fx:id="lblPeak" alignment="CENTER" style="-fx-font-size: 12;" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</HBox>
